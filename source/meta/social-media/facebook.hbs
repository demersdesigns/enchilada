<!-- Start of Facebook Open Graph -->
<!-- https://developers.facebook.com/docs/sharing/webmasters#markup -->
<!-- http://ogp.me/ -->
<meta property="og:site_name" content="{{site.projectFullTitle}}"/>
<meta property="og:description" content="{{site.projectShortDescription}}"/>
<meta property="og:url" content="http://{{site.projectURL}}/"/>
<meta property="og:type" content="website"/>
<meta property="og:locale" content="{{site.projectLanguage}}">
<meta property="og:image" content="http://{{site.projectURL}}/static/favicons/social/facebook.png?rev={{timestamp}}" />
<meta property="og:image:width" content="599"/>
<meta property="og:image:height" content="314"/>
<!-- End of Facebook Open Graph -->
